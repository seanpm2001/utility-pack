{
    "name": "skyzyx/utility-pack",
    "description": "A set of small utilities for PHP.",
    "keywords": ["php", "time", "querystring", "types", "url"],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ryan Parman",
            "homepage": "http://ryanparman.com"
        }
    ],
    "support": {
        "issues": "https://github.com/skyzyx/utility-pack/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "friendsofphp/php-cs-fixer": "^2.3",
        "pdepend/pdepend": "^2.5",
        "phploc/phploc": "^3.0",
        "phpmetrics/phpmetrics": "^2.2",
        "sebastian/phpcpd": "^3.0",
        "skyzyx/phpunit-result-printer": "^2.0",
        "squizlabs/php_codesniffer": "^3.0",
        "wapmorgan/php-code-analyzer": "^1.0",
        "qafoo/quality-analyzer": "dev-master"
    },
    "config": {
        "bin-dir": "bin/",
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Skyzyx\\UtilityPack\\": "src/",
            "Skyzyx\\Tests\\UtilityPack\\": "tests/"
        }
    }
}
